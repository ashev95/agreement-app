						<title>корпус</title>
						<view>корпус</view>
						<type>text</type>
						<editable>true</editable>
					</attribute>
					<attribute>
						<code>building</code>
						<title>строение</title>
						<view>строение</view>
						<type>text</type>
						<editable>true</editable>
					</attribute>
					<attribute>
						<code>door</code>
