		<form key="agreement">
			<attributeContainer>
				<attributes>
					<attribute>
						<code>id</code>
						<title>Идентификатор договора</title>
						<view>Идентификатор</view>
						<type>id</type>
						<editable>true</editable>
					</attribute>
					<attribute>
						<code>insuranceAmount</code>
						<title>Страховая сумма</title>
						<view>Страховая сумма</view>
						<type>number</type>
						<editable>true</editable>
					</attribute>
					<attribute class="SelectAttribute">
						<code>typeOfProperty</code>
						<title>Тип недвижимости</title>
						<view>Тип недвижимости</view>
						<type>select</type>
						<options>
							<option>Квартира</option>
							<option>Дом</option>
							<option>Комната</option>
						</options>
						<editable>true</editable>
					</attribute>
					<attribute class="SelectAttribute">
						<code>yearOfConstruction</code>
						<title>Год постройки</title>
						<view>Год постройки</view>
						<type>select</type>
						<options>
							<option>2020</option>
							<option>2019</option>
							<option>2018</option>
							<option>2017</option>
							<option>2016</option>
							<option>2015</option>
							<option>2014</option>
							<option>2013</option>
						</options>
						<editable>true</editable>
					</attribute>
					<attribute>
